  "license": "ISC",
  "devDependencies": {
    "asar": "^0.12.3",
    "electron-packager": "^8.2.0",
    "electron-prebuilt": "^1.4.6",
    "rimraf": "^2.5.4"
  },
  "dependencies": {
    "bootstrap": "^3.3.7",
    "inputmask": "^3.3.7",
    "jquery": "^3.2.1",
    "sweetalert": "^1.1.3"
  }
